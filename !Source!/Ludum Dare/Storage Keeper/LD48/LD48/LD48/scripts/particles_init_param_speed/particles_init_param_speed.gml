function particles_init_param_speed(start_min_x, start_max_x, start_min_y, start_max_y, end_min_x, end_max_x, end_min_y, end_max_y) {
    particles_speed_start_min_x = start_min_x;
    particles_speed_start_max_x = start_max_x;
    particles_speed_start_min_y = start_min_y;
    particles_speed_start_max_y = start_max_y;
    particles_speed_end_min_x = end_min_x;
    particles_speed_end_min_y = end_min_y;
    particles_speed_end_max_x = end_max_x;
    particles_speed_end_max_y = end_max_y;
}